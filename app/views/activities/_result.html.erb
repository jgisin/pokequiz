<% present act.user do |user_presenter| %>
    <div class="row">
      <% if user_presenter.user_thumbnail %>
          <div class="col-xs-4">
            <%= user_presenter.user_thumbnail %>
          </div>
      <% end %>
      <div class="col-xs-8">
        <%= user_presenter.user_show_link %> got a question <%= user_presenter.question_right(act.event) %>
      </div>
    </div>
<% end %>
<br/>
